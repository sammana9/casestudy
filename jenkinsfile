def CODE_CHANGES
pipeline {
    agent any

    stages {
        stage('codereview') {
		  when {
  allOf {
    branch 'master'
    changeRequest url: 'https://github.com/sammana9/casestudy'
  }
}
            steps {
                echo 'code review successfully done'
            }
        }
         stage('unittest') {
            steps {
                echo 'code review successfully done'
            }
        }
         stage('codecoverage') {
            steps {
                echo 'code review successfully done'
            }
        }
         stage('package') {
            steps {
                echo 'code review successfully done'
            }
        }
    }
}
