pipeline {
    agent any

    stages {
        stage('codereview') {
		  when {
                    expression{
		    env.GIT_BRANCH == 'origin/master'
		    }
}

            steps {
                echo 'code review successfully done'
		echo "${env.BRANCH_NAME}"
            }
        }
         stage('unittest') {
            steps {
                echo 'code review successfully done'
            }
        }
         stage('codecoverage') {
            steps {
                echo 'code review successfully done'
            }
        }
         stage('package') {
            steps {
                echo 'code review successfully done'
            }
        }
    }
}
