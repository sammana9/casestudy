def CODE_CHANGES
pipeline {
    agent any

    stages {
        stage('codereview') {
		when {
		 expression
		 {
		  BRANCH_NAME=='master' && CODE_CHANGES==true
		 }
		 
		}
            steps {
                echo 'code review successfully done'
            }
        }
         stage('unittest') {
            steps {
                echo 'code review successfully done'
            }
        }
         stage('codecoverage') {
            steps {
                echo 'code review successfully done'
            }
        }
         stage('package') {
            steps {
                echo 'code review successfully done'
            }
        }
    }
}
