def CODE_CHANGES
pipeline {
    agent any

    stages {
        stage('codereview') {
		  when
		  {
                   branch 'master'
		   }

            steps {
                echo 'code review successfully done'
            }
        }
         stage('unittest') {
            steps {
                echo 'code review successfully done'
            }
        }
         stage('codecoverage') {
            steps {
                echo 'code review successfully done'
            }
        }
         stage('package') {
            steps {
                echo 'code review successfully done'
            }
        }
    }
}
